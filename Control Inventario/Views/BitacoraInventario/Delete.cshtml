@model ControlInventarioModel.BitacoraInventario

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>BitacoraInventario</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Ubicación (No. de Gondola)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UbicacionId)
        </dd>
        <dt class = "col-sm-2">
            Fecha de ingreso
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaIngreso)
        </dd>
        <dt class = "col-sm-2">
            Fecha de modificación
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FechaModificacion)
        </dd>
        <dt class = "col-sm-2">
            Nombre del producto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProductoId)
        </dd>
        <dt class = "col-sm-2">
            Cantidad Inicial
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CantidadInicial)
        </dd>
        <dt class = "col-sm-2">
            Cantidad Actual
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CantidadActual)
        </dd>
        <dt class = "col-sm-2">
            Disponibilidad
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Disponibilidad)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
